<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include user preferences -->
        <include domain="sharedpref" path="." />
        
        <!-- Exclude sensitive data -->
        <exclude domain="sharedpref" path="root_status.xml" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Include user preferences -->
        <include domain="sharedpref" path="." />
        
        <!-- Exclude sensitive data -->
        <exclude domain="sharedpref" path="root_status.xml" />
    </device-transfer>
</data-extraction-rules>
